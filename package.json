{
  "name": "v-model",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "moment": "^2.22.2",
    "vue": "^2.5.16",
    "vue-nav-tabs": "^0.5.7",
    "vue2-datepicker": "^2.0.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "jest": "^23.4.2",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "babel": {
    "presets": [[
      "env",
      {
        "targets:": {
          "node": "6"
        }
      }
    ]]
  },
  "jest": {
    "moduleFileExtensions": [
      "es6",
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/*Spec.(es6|js)"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.(es6|js)$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
